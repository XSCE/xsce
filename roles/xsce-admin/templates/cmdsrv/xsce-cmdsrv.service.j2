[Unit]
Description=Provides the XSCE Command Server
Requires=httpd.service
After=syslog.target network.target local-fs.target httpd.service

[Service]
Type=forking
PIDFile=/var/run/xsce-cmdsrv.pid
ExecStart=/usr/libexec/xsce-cmdsrv-init  start 
ExecStop=/usr/bin/xsce-cmdsrv-ctl STOP

[Install]
WantedBy=multi-user.target
